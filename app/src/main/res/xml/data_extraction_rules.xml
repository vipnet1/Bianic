<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <include
            domain="sharedpref"
            path="." />
        <include
            domain="database"
            path="." />
        <exclude
            domain="sharedpref"
            path="encrypted_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="sharedpref"
            path="." />
        <include
            domain="database"
            path="." />
        <exclude
            domain="sharedpref"
            path="encrypted_prefs.xml" />
    </device-transfer>
</data-extraction-rules>